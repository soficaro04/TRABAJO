variable1=int(input("dame el primer numero"))
variable2=int(input("dame el segundo numero"))

suma=variable1+variable2
print(f'el resultado de la suma es {suma}')

resta=variable1-variable2
print(f'el resultado de la resta es {resta}')

multiplicacion=variable1*variable2
print(f'el resultado de la multiplicacion es {multiplicacion}')

division=variable1/variable2
print(f'el resultado de la division es {division}')

modulo=variable1 % variable2
print(f'el resultado del modulo es {modulo}')

